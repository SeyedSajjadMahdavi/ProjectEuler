def Is_Palindrom(number):
    n=str(number).zfill(6)
    if n[0]==n[5] and\
        n[1]==n[4] and\
        n[2]==n[3]:
        return True
    else:
        return False
import time
start=time.time()
List=[]
for i in range(999,99,-1):
    for j in range(999,99,-1):
        number=i*j
        if Is_Palindrom(number):
            List.append(number)
            break
print(max(List))
end=time.time()
print('Running time is %s second'% round(end-start, 5))
